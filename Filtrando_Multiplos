#include <iostream>

using namespace std;

int main()
{

    int N,K;

    cin>>N;

    int A[N];

    for(int i=0;i<=(N-1);i++)
    {
        cin>>A[i];
    }

    cin>>K;

    for(int j=0;j<=(N-1);j++)
    {

        if(A[j] % K==false)
        {
            cout<<A[j]<<" ";
        }
        else
        {
            cout<<"X ";
        }
    }

    return 0;
}
